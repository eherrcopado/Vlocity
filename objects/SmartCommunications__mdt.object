<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This Custom Metadata Type holds OAuth2.0 Password Grant details</description>
    <fields>
        <fullName>accessTokenUrl__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Token Endpoint URL</label>
        <required>true</required>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>clientId__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Consumer Key</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>clientSecret__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Consumer Secret</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>credsInHeader__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Send client credentials in header</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>password__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Password</label>
        <length>255</length>
        <required>true</required>
        <securityClassification>Confidential</securityClassification>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>redirectUri__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Callback URL</label>
        <required>true</required>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>scope__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Default Scopes</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>username__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>username</label>
        <length>255</length>
        <required>true</required>
        <securityClassification>Confidential</securityClassification>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>SmartCommunications</label>
    <pluralLabel>SmartCommunications</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
